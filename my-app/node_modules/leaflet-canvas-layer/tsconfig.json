{
    "compilerOptions": {
        //"outDir": "./dist",
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "sourceMap": true,
        "noEmitHelpers": false,
        "noImplicitAny": true,
        "declaration": true,
        "declarationDir": "dist",
        "skipLibCheck": true,
        "stripInternal": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "lib": ["dom", "es2016"]
    }
}